#header
 =image_tag 'header2.png'
#section
 .get-started-css
  %h1="Login"
  
  - form_for @user_session do |f|
   %div{:class=>"field"}
    = f.label :username
    = f.text_field :username
   %div{:class=>"field"}
    = f.label :password
    = f.password_field :password
   %div{:class=>"actions"}
    = f.submit "Login"
    